<Window x:Class="WpfApp5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp5"
        mc:Ignorable="d"
        Title="Вычислите куб большего из чисел" Height="350" Width="300">
    <DockPanel LastChildFill="True" VerticalAlignment="Center" Margin="10">
        <Label Content="Продолжительность одной минуты разговора:" DockPanel.Dock="Top"/>
        <TextBox x:Name="x1" DockPanel.Dock="Top"/>
        <Label Content="Стоимость одной минуты разговора:" DockPanel.Dock="Top"/>
        <TextBox x:Name="x2" DockPanel.Dock="Top"/>
        <Button Content="Вычислить общую стоимость разговора:" DockPanel.Dock="Top" Click="Button_Click"/>
        <TextBlock x:Name="end" DockPanel.Dock="Bottom"/>
    </DockPanel>
</Window>
